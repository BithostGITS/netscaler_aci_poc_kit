            <vnsAbsConnection name="CON2">
                <vnsRsAbsConnectionConns tDn="uni/tn-tenant_parik1/AbsGraph-WebGraph/AbsNode-Node1/AbsFConn-inside"/>
                <!-- <vnsRsAbsConnectionConns tDn="uni/tn-tenant_parik1/AbsGraph-WebGraph/AbsTermNodeProv-Input1/AbsTConn"/> -->
