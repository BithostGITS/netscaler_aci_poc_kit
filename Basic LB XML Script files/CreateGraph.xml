            </vnsAbsTermNodeProv>

            <!-- Node1 Provides LoadBalancing functionality -->
            <vnsAbsNode name="Node1"
                        funcType="GoTo"
                        >
                <vnsRsDefaultScopeToTerm tDn="uni/tn-tenant_parik1/AbsGraph-WebGraph/AbsTermNodeProv-Input1/outtmnl"/>

                <vnsAbsFuncConn name="outside"
